<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression Suite" verbose="1">
    <listeners>
        <listener class-name="listeners.AttachmentListener"/>
    </listeners>

    <test name="Regression test">
        <classes>
            <class name="regression.LoginTest"/>
            <class name="regression.CaseDuplicateTest"/>
            <class name="regression.PageAdditionToWorkspaceTest"/>
            <class name="regression.PageStaplingTest"/>
            <class name="regression.BasicEpisodeDataPopulationTest"/>
            <class name="regression.EpisodeNotesAdditionTest"/>
            <class name="regression.PageColorAdditionTest"/>
            <class name="regression.PageTagAdditionTest"/>
        </classes>
    </test>
</suite>